<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %} {{ 'Short URL' | trans }} {% endblock %}</title>

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,400;0,700;1,100;1,400;1,700&display=swap" rel="stylesheet">

    <link href="{{ asset('asset') }}/lib/bootstrap/css/simple-sidebar.css" rel="stylesheet" />
    <link href="{{ asset('asset') }}/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="{{ asset('asset') }}/lib/fontawsome/css/all.min.css" rel="stylesheet" />

    {# Run `composer require symfony/webpack-encore-bundle`
       and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        {#{{ encore_entry_link_tags('app') }}#}
    {% endblock %}
    {% block javascripts %}
        {#{{ encore_entry_script_tags('app') }}#}
    {% endblock %}

</head>
<body>






<div class="d-flex" id="wrapper">

    <!-- Sidebar -->
    {% block sidebar %}{% include 'admin/inc/sidebar.html.twig' %}{% endblock %}

    <!-- /#sidebar-wrapper -->
    <!-- Page Content -->
    <div id="page-content-wrapper">

        {% block header %}{% include 'admin/inc/header.html.twig' %}{% endblock %}

        <div class="container-fluid">



            {% block body %}   {% endblock %}


        </div>
    </div>
    <!-- /#page-content-wrapper -->

</div>








{% block footer %} {% endblock %}

<script src="{{ asset('asset') }}/lib/jquery/jquery.min.js"></script>
<script src="{{ asset('asset') }}/lib/bootstrap/js/bootstrap.min.js"></script>
<script src="{{ asset('asset') }}/lib/fontawsome/js/all.min.js"></script>
<script>
    $("#menu-toggle").click(function (e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");

    });
</script>

</body>
</html>
